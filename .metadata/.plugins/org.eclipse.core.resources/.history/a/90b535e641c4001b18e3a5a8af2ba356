package com.springbasic.SpringBasic;

public class B {
	private String name;
	B()
	{
		System.out.println("the value of id is "+name);
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public B(String name) {
		super();
		this.name = name;
	}
	

}
